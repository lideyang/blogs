{
  "name": "lidy-blog",
  "version": "1.0.0",
  "private": true,
  "description": "lidy-blog for express react",
  "scripts": {
    "start": "npm-run-all --sequential dev build:client",
    "dev": "npm-run-all --sequential build:dll build:server start-dev",
    "build:dll": "webpack --config webpack.dll.config.js",
    "build:server": "webpack --colors --config webpack.config.dev.server.js --watch",
    "build:client": "webpack --colors --config webpack.config.dev.client.js",
    "build:prod": "webpack --colors --config webpack.config.prod.js",
    "start-dev": "node server.js"
  },
